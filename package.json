{
  "private": true,
  "name": "advent-of-code-2022",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "preact build",
    "build:gh": "GITHUB_PAGES=advent-of-code-2022 preact build && dot-json ./build/manifest.json start_url \"/advent-of-code-2022/\"",
    "serve": "sirv build --port 8080 --cors --single",
    "dev": "preact watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json,css,prettierrc,eslintrc,html,babelrc}\"",
    "check:format": "prettier --check \"**/*.{js,jsx,ts,tsx,md,json,css,prettierrc,eslintrc,html,babelrc}\"",
    "check": "npm run type-check && npm run lint && npm run check:format"
  },
  "dependencies": {
    "@ungap/structured-clone": "^1.0.1",
    "feather-icons": "^4.29.0",
    "history": "^4.10.1",
    "mergerino": "^0.4.0",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6",
    "preact-router": "^3.2.1",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.29.0",
    "sanitize.css": "^13.0.0",
    "staterino": "^2.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.12",
    "@types/feather-icons": "^4.29.0",
    "@types/history": "^4.7.11",
    "@types/node": "^18.11.11",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "dot-json": "^1.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^4.0.1",
    "eslint": "^8.29.0",
    "eslint-config-adpyke-es6": "^1.4.13",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "jest": "^29.3.1",
    "preact-cli": "^3.4.1",
    "prettier": "^2.8.0",
    "sirv-cli": "^2.0.2",
    "typescript": "^4.9.3"
  },
  "overrides": {
    "ansi-regex": "^5.0.1",
    "glob-parent": "^6.0.1",
    "nth-check": "^2.0.1",
    "json-schema": "^0.4.0",
    "jsdom": "^16.4.0",
    "got": "^11.8.5",
    "loader-utils": "^2.0.4"
  }
}
